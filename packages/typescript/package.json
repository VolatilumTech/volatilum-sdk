{
  "name": "@volatilum/sdk-typescript",
  "version": "0.1.0",
  "description": "Read-only, deterministic interface to Volatilum liquidity infrastructure (Solana).",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node --test dist/**/*.test.js"
  },
  "dependencies": {
    "@solana/web3.js": "1.98.4"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "typescript": "5.4.5"
  }
}
